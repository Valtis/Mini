<p id="notice"><%= notice %></p>

<h1>Listing Images</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |image| %>
      <tr>

        <%= link_to "#{image_tag(image.S3Image.url(:medium))}".html_safe, image if has_right_to_see_image(image) %>
      </tr>
      <br/>
    <% end %>
  </tbody>
</table>

<br>



<%= link_to 'New Image', new_image_path %>